void loop() {
  
  

  delay(100);
  while(Serial.available()){
    oled.clearDisplay();
    oled.setCursor(0,0);
    String temp=Serial.readString();
    for(int j=0;j<temp.length();j++)
    if(temp[j]=='/'){
      oled.setTextSize(temp[j+1]-48);
      
      temp[j]='\0';
      temp[j+1]='\0';
    }
    for(int j=0;j<temp.length();j++)
    if(temp[j]=='\\'){
      temp[j]=219+temp[j+1]-48;
      temp[j+1]='\0';
    }

    for(int j=0;j<temp.length();j++)
    if(temp[j]=='\\'){
      temp[j]=219+temp[j+1]-48;
      temp[j+1]='\0';
    }
    oled.print(temp);
    Serial.print(temp);
    oled.display();
    delay(1000);
    
    oled.display();
  }
